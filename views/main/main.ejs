<div class="row">
  <div class="col-lg-12 grid-margin stretch-card">
    <div class="card">
      <div class="card-body">
        <h4 class="card-title">전체 블럭</h4>
        <p class="card-description">
          현재 라이브 중인 모든 블럭입니다<code></code>
        </p>
        <table class="table table-striped">
          <thead>
            <tr>
              <th>
                Shortcut
              </th>
              <th>
                Block name
              </th>
              <th>
                Author
              </th>
              <th>
                Status
              </th>
              <th>
                Sketch File
              </th>
              <th>
                Revision
              </th>
              <th>
                Update
              </th>
              <th>
                상세
              </th>
            </tr>
          </thead>
          <tbody>
            <% for (var i = 0; i < list.length; i++) { %>

              <%
                var targetComponent = "NOT YET";
                var entityRule = list[i]['entityRule'];
                for (var j = 0; j < entityRule.length; j++) {
                  if(entityRule[j]['type'] == 'targetComponent')
                    targetComponent = entityRule[j]['value']['select2'];
                }
              %>
                <tr>
                  <td class="py-1">
                    <img src="file/<%=list[i]['key']%>/<%=list[i]['revision']%>/previews/preview.png" alt="image"/>
                  </td>
                  <td>
                    <a href="/detail?key=<%=list[i]['key']%>&revision=<%=list[i]['revision']%>">
                      <%=targetComponent%>
                    </a>
                  </td>
                  <td>
                    Mr.Kim
                  </td>
                  <td>
                    <label class="badge badge-success">Live</label>
                  </td>
                  <td>
                    <a href="/downloadSketch?key=<%=list[i]['key']%>&revision=<%=list[i]['revision']%>">
                      <i class="fa fa-cloud-download"></i> Download
                    </a>
                  </td>
                  <td>
                    4
                  </td>
                  <td>
                    <%=new Date(Number.parseInt(list[i]['update'])).toISOString();%>
                  </td>
                  <td>
                    <a href="/detail?key=<%=list[i]['key']%>&revision=<%=list[i]['revision']%>">
                    <button type="submit" class="btn btn-secondary btn-fw">블록상세>></button>
                    </a>
                  </td>

                </tr>
            <%}%>
          </tbody>
        </table>
      </div>
    </div>
  </div>
</div>
