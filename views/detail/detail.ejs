
<link rel="stylesheet" type="text/css" href="js/jsontree/jsoneditor.css"/>
<script src="js/jsontree/jquery.jsoneditor.js"></script>

<div class="row">
  <div class="col-lg-4 grid-margin">
    <div class="card">
      <div class="card-body">
        <h4 class="card-title">미리보기</h4>
        <div class="icons-list row">
          <div class="col-sm-8 col-md-4 col-lg-4"> <i class="fa fa-address-book"></i></div>
          <div class="col-sm-8 col-md-4 col-lg-4"> <i class="fa fa-address-card"></i></div>
          <div class="col-sm-8 col-md-4 col-lg-4"> <i class="fa fa-address-book-o"></i></div>
          <div class="col-sm-8 col-md-4 col-lg-4"> <i class="fa fa-address-card-o"></i></div>
          <div class="col-sm-8 col-md-4 col-lg-4"> <i class="fa fa-bandcamp"></i> </div>
        </div>
      </div>
    </div>
  </div>

  <div class="col-lg-4 grid-margin">
    <div class="card">
      <div class="card-body">
        <h4 class="card-title">Cloud Json</h4>

        <div id='editor_holder' class='json-editor'></div>

        <script>
          var myjson = { any: { json: { value: 1 } } };
          var opt = {
              change: function(data) { /* called on every change */ },
              propertyclick: function(path) { /* called when a property is clicked with the JS path to that property */ }
          };
          /* opt.propertyElement = '<textarea>'; */ // element of the property field, <input> is default
          /* opt.valueElement = '<textarea>'; */  // element of the value field, <input> is default
          $('#editor_holder').jsonEditor(myjson, opt);
        </script>
      </div>
    </div>
  </div>

  <div class="col-lg-4 grid-margin">
    <div class="card">
      <div class="card-body">
        <h4 class="card-title">대응 Data API</h4>
        <div class="icons-list row">
          <div class="col-sm-8 col-md-4 col-lg-4"> <i class="fa fa-address-book"></i></div>
          <div class="col-sm-8 col-md-4 col-lg-4"> <i class="fa fa-address-card"></i></div>
          <div class="col-sm-8 col-md-4 col-lg-4"> <i class="fa fa-address-book-o"></i></div>
          <div class="col-sm-8 col-md-4 col-lg-4"> <i class="fa fa-address-card-o"></i></div>
          <div class="col-sm-8 col-md-4 col-lg-4"> <i class="fa fa-bandcamp"></i> </div>
        </div>
      </div>
    </div>
  </div>
</div>

